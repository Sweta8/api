package stringPrograms;

public class DivideIntoNEquaParts {
	
	public static void main(String[] args)
	{
		String x="Sweta is singing";
		int n=3;
		String y="";
		if(x.length()/n==0)
		{
			System.out.println("String can be divided into n equal parts");
		}
		else
		{
			for(int i=0;i<x.length();i++)
			{
				 y=y+" "+x.charAt(i);
			}
		}
	}

}
