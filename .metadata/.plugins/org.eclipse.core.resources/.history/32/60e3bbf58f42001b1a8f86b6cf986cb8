package stringPrograms;

public class minandmaxcharacterinstring {
	
	public static void main(String[] args)
	{
		String x="Max is sweta";
		x=x.toLowerCase();
		char [] ch=x.toCharArray();
		for(int i=0;i<ch.length;i++)
		{
			int k=1;
			for(int j=i+1;j<ch.length;j++)
			{
				if(ch[i]==ch[j])
				{
					k++;
					ch[j]=' ';
				}
			}
		}
	}

}
