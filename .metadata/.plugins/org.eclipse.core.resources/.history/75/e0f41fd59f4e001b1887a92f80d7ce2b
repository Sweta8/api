package stringPrograms;

public class LargestAndSmallestPalindrome {
	
	public static void main(String[] args)
	{
		String x="mmadamm madam";
		String y[]=x.split(" ");
		String l="";
		int reverseLength=0;
		for(int i=0;i<y.length;i++)
		{
			String z=y[i];
			for(int j=y[i].length()-1;j>=0;j--)
			{
				l=l+y[i].charAt(j);
			}
			if(z.equalsIgnoreCase(l))
			{
				
			}
		}
	}

}
